<aiml version="1.0.1" encoding="UTF-8">
    <!-- std-startup.xml -->

    <!-- Category is an atomic AIML unit -->
    <category>

        <!-- Pattern to match in user input -->
        <!-- If user enters "LOAD AIML B" -->
        <pattern>LOAD AIML</pattern>

        <!-- Template is the response to the pattern -->
        <!-- This learn an aiml file -->
        <template>
            <learn>basic_chat.xml</learn>
            <!-- You can add more aiml files here -->
            <!--<learn>more_aiml.aiml</learn>-->
        </template>

    </category>
    <category>
        <pattern>LOAD</pattern>
        <template>
            <learn>AIML_set/general.aiml</learn>
            <learn>AIML_set/learn.aiml</learn>
            <learn>AIML_set/maimeng.aiml</learn>
            <learn>AIML_set/property.aiml</learn>
            <learn>AIML_set/rude.aiml</learn>
            <learn>AIML_set/sex.aiml</learn>
        </template>

    </category>

</aiml>
